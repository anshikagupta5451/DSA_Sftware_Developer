
class Main {
    public static boolean checkSorted(int arr[]){
        int n=arr.length-1;
        for(int i=0;i<n;i++){
            if(arr[i+1]<arr[i])
            return false;
        }
        return true;
    }
    public static void main(String[] args) {
        int arr[]={1,2,3,4,5,6,7,8,8};
        Boolean isSorted=checkSorted(arr);
        System.out.println("isSorted "+isSorted);
    }
}
